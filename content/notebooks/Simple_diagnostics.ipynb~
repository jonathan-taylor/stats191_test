   "source": [
    "plot(X, Y, pch = 23, bg = \"red\", main=\"Error sum of squares\", cex=2)\n",
    "abline(Y.lm, col = \"green\", lwd = 2)\n",
    "for (i in 1:21) {\n",
    "    points(X[i], Yhat[i], pch = 23, bg = \"green\")\n",
    "    lines(c(X[i], X[i]), c(Y[i], Yhat[i]))\n",
    "}\n",
    "abline(h = meanY, col = \"yellow\", lwd = 2)"
   ]
