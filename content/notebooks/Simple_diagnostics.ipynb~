    "quadratic.lm = lm(y ~ poly(x, 2))\n",
    "Xsort = sort(x)\n",
    "plot(x, y, pch = 23, bg = \"orange\", cex = 2, ylab = \"Y\",\n",
    "     xlab = \"X\")\n",
    "lines(Xsort, predict(quadratic.lm, list(x = Xsort)), col = \"red\", lty = 2,\n",
    "      lwd = 2)"
