    "plot(x, resid(quadratic.lm), ylab = \"Residual\", xlab = \"X\", pch = 23,\n",
    "        bg = \"orange\", cex = 2)\n",
    "abline(h = 0, lwd = 2, col = \"red\", lty = 2)"
