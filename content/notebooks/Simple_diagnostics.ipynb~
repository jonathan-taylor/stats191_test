   "source": [
    "plot(X, Y, pch = 23, bg = \"red\", main='Total sum of squares', cex=2)\n",
    "abline(Y.lm, pch=23, col='green', lwd=2)\n",
    "abline(h = meanY, col = \"yellow\", lwd = 2)\n",
    "for (i in 1:21) {\n",
    "      points(X[i], meanY, pch = 23, bg = \"yellow\")\n",
    "      lines(c(X[i], X[i]), c(Y[i], meanY))\n",
    "}\n"
   ]
  },
