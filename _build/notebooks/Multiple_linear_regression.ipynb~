   "source": [
    "n = length(Y)\n",
    "attach(prostate)\n",
    "X = cbind(rep(1,n), lcavol, lweight, age, lbph, svi, lcp, pgg45)\n",
    "detach(prostate)\n",
    "colnames(X)[1] = '(Intercept)'\n",
    "head(X)\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
    "slideshow": {
     "slide_type": "slide"
    }
   },
   "source": [
    "The matrix X is the same as formed by `R`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "collapsed": false,
    "slideshow": {
     "slide_type": "fragment"
    }
   },
   "outputs": [
    {
     "data": {
      "text/html": [
       "<table>\n",
       "<thead><tr><th></th><th scope=col>(Intercept)</th><th scope=col>lcavol</th><th scope=col>lweight</th><th scope=col>age</th><th scope=col>lbph</th><th scope=col>svi</th><th scope=col>lcp</th><th scope=col>pgg45</th></tr></thead>\n",
