    "X = rnorm(50)\n",
    "Y = 1.5 + 0.1 * X + rnorm(50) * 2\n",
    "parameters = lm(Y ~ X)$coef\n",
    "intercept = parameters[1]\n",
    "slope = parameters[2]\n",
    "ggplot(data.frame(X, Y), aes(X, Y)) + geom_point() + geom_abline(slope=slope, intercept=intercept)"
